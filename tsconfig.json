{
    "compilerOptions": { "incremental": true,                              
      "target": "es2016",
      "module": "commonjs",
      "outDir": "./dist",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "rootDirs": ["src", "test"],
      "baseUrl": "src",
      "paths": {
        "@/*": ["*"],
        "@/test/*": ["../test/*"]
      },
    }
  }